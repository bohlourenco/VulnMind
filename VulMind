###  http://www.w3.org/2002/07/owl#topDataProperty
owl:topDataProperty rdfs:subPropertyOf owl:topDataProperty .


#################################################################
#    Classes
#################################################################

###  http://purl.org/cyber/cve#CVE
<http://purl.org/cyber/cve#CVE> rdfs:subClassOf <http://purl.org/cyber/cve#Vulnerability> ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty <http://purl.org/cyber/cve#affectsProduct> ;
                                                  owl:someValuesFrom <http://purl.org/cyber/cve#Product>
                                                ] ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :affectsPackage ;
                                                  owl:someValuesFrom :Package
                                                ] ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :identifiesVulnerability ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onClass <http://purl.org/cyber/cve#Vulnerability>
                                                ] ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasCVEID ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onDataRange xsd:string
                                                ] ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasDescription ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onDataRange xsd:string
                                                ] ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasPublishedDate ;
                                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                  owl:onDataRange xsd:string
                                                ] ;
                                rdfs:comment "A publicly identified vulnerability (Common Vulnerabilities and Exposures) used to reference exploitable weaknesses and to correlate vulnerabilities with affected assets and evidence." .


###  http://purl.org/cyber/cve#Product
<http://purl.org/cyber/cve#Product> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                      owl:onProperty <http://purl.org/cyber/cve#hasVendor> ;
                                                      owl:someValuesFrom <http://purl.org/cyber/cve#Vendor>
                                                    ] ,
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasProduct ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onDataRange owl:rational
                                                    ] ;
                                    rdfs:comment """Represents the upstream software product that a package delivers or corresponds to.
It is a more abstract entity than a package.""" .


