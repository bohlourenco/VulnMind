             rdfs:subPropertyOf owl:topDataProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :KernelEvent
                                         :LogEvent
                                       )
                         ] ;
             rdfs:range xsd:string ;
             rdfs:comment "Severity text identifying the textual name of the severity (e.g., \"emergency\", \"alert\", \"critical\", \"error\", \"warning\", \"notice\", \"informational\", \"debug\")." .


###  http://www.semanticweb.org/ontology/2025/semlogonto/hasSeverityCode
:hasSeverityCode rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdfs:domain :LogEvent ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :hasSeverityCode ;
                               owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                   owl:oneOf [ rdf:type rdf:List ;
                                                               rdf:first "DEBUG" ;
                                                               rdf:rest [ rdf:type rdf:List ;
                                                                          rdf:first "ERROR" ;
                                                                          rdf:rest [ rdf:type rdf:List ;
                                                                                     rdf:first "FATAL" ;
                                                                                     rdf:rest [ rdf:type rdf:List ;
                                                                                                rdf:first "INFO" ;
                                                                                                rdf:rest [ rdf:type rdf:List ;
                                                                                                           rdf:first "TRACE" ;
                                                                                                           rdf:rest [ rdf:type rdf:List ;
                                                                                                                      rdf:first "WARN" ;
                                                                                                                      rdf:rest rdf:nil
                                                                                                                    ]
                                                                                                         ]
                                                                                              ]
                                                                                   ]
                                                                        ]
                                                             ]
                                                 ]
                             ] ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Severity code identifying the severity of the message (e.g., 0 = Emergency, 1 = Alert, 2 = Critical, 3 = Error, 4 = Warning, 5 = Notice, 6 = Informational, 7 = Debug)." .


