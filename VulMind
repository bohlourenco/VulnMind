###  http://www.semanticweb.org/ontology/2025/semlogonto/hasNextEntry
:hasNextEntry rdf:type owl:ObjectProperty ;
              owl:inverseOf :hasPreviousEntry ;
              rdf:type owl:FunctionalProperty ,
                       owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty ;
              rdfs:domain :ApplicationLogEvent ,
                          :LogEvent ,
                          :SystemLogEvent ;
              rdfs:range :ApplicationLogEvent ,
                         :LogEvent ,
                         :SystemLogEvent ;
              rdfs:comment "Relates an entry in a sequence (e.g., log or history) to the immediately following entry." .


