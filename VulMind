@prefix : <http://www.semanticweb.org/ontology/2025/semlogonto/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/ontology/2025/semlogonto/> .

<http://www.semanticweb.org/ontology/2025/semlogonto> rdf:type owl:Ontology ;
                                                       owl:imports <http://purl.org/cyber/cve> ,
                                                                   <http://xmlns.com/foaf/0.1/> .

#################################################################
#    Object Properties
#################################################################

###  http://purl.org/cyber/cve#affectsProduct
<http://purl.org/cyber/cve#affectsProduct> rdfs:subPropertyOf owl:topObjectProperty ;
                                           owl:inverseOf :isAffectedBy ;
                                           rdfs:domain [ rdf:type owl:Class ;
                                                         owl:unionOf ( <http://purl.org/cyber/cve#CVE>
                                                                       <http://purl.org/cyber/cve#Vulnerability>
                                                                     )
                                                       ] ;
                                           rdfs:range <http://purl.org/cyber/cve#Product> ;
                                           owl:propertyChainAxiom ( :affectsPackage
                                                                  ) ;
                                           rdfs:comment "Indicates that a CVE or vulnerability affects a specific product (software, hardware, or system). This property is used when the vulnerability is confirmed to impact a product as defined in asset inventories, SBOMs, or configuration data." ,
                                                        "Indicates that a vulnerability or CVE affects a product (software, hardware, or composite system) as a whole." .


###  http://purl.org/cyber/cve#hasAttacker
<http://purl.org/cyber/cve#hasAttacker> rdfs:subPropertyOf owl:topObjectProperty ;
                                        rdfs:comment "Associates an attack, incident, or threat event with the attacker entity (threat actor, group, or toolset) responsible for it." .


###  http://purl.org/cyber/cve#hasCVE
<http://purl.org/cyber/cve#hasCVE> rdfs:subPropertyOf owl:topObjectProperty ;
                                   owl:inverseOf :identifiesVulnerability ;
                                   rdfs:domain <http://purl.org/cyber/cve#Vulnerability> ;
                                   rdfs:range <http://purl.org/cyber/cve#CVE> ;
                                   rdfs:comment "Associates a vulnerability with its corresponding CVE identifier. Used to reference external vulnerability catalogs and standardized naming schemes (e.g., NVD, MITRE). A vulnerability may have zero, one, or multiple CVE identifiers." .


###  http://purl.org/cyber/cve#hasProduct
<http://purl.org/cyber/cve#hasProduct> rdfs:subPropertyOf owl:topObjectProperty ;
                                       rdfs:comment "Links a vendor, portfolio, host, or system to a product that it manufactures, offers, or contains." .


###  http://purl.org/cyber/cve#hasReferences
<http://purl.org/cyber/cve#hasReferences> rdfs:subPropertyOf owl:topObjectProperty ;
                                          rdfs:comment "Associates an entity (typically a CVE, vulnerability, or report) with external reference resources such as advisories, bulletins, or documentation." .


###  http://purl.org/cyber/cve#hasVendor
<http://purl.org/cyber/cve#hasVendor> rdfs:subPropertyOf owl:topObjectProperty ;
                                      rdfs:comment "Links a product or package to the vendor or organization that develops, distributes, or maintains it." .


###  http://purl.org/cyber/cve#hasVulnerability
<http://purl.org/cyber/cve#hasVulnerability> rdfs:subPropertyOf owl:topObjectProperty ;
                                             rdfs:domain <http://purl.org/cyber/cve#Vulnerability> ,
                                                         :Asset ;
                                             rdfs:range <http://purl.org/cyber/cve#CVE> ;
                                             rdfs:comment "Associates an asset (product, host, package, or system) with a vulnerability known to affect it." .


###  http://www.semanticweb.org/ontology/2025/semlogonto#hasAffectedProduct
:hasAffectedProduct rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/ontology/2025/semlogonto#hasSeverityScore
:hasSeverityScore rdf:type owl:ObjectProperty ;
                  rdfs:domain <http://purl.org/cyber/cve#Vulnerability> ;
                  rdfs:range :CVSSSeverity .


###  http://www.semanticweb.org/ontology/2025/semlogonto/acesses
:acesses rdf:type owl:ObjectProperty ;
         rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
         rdfs:range :Asset ;
         rdfs:comment "Attempts or performs access (successful or not)." .


###  http://www.semanticweb.org/ontology/2025/semlogonto/administers
:administers rdf:type owl:ObjectProperty ;
             rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
             rdfs:range :Asset ;
             rdfs:comment "Agent has administrative control." .


###  http://www.semanticweb.org/ontology/2025/semlogonto/affectsHostProduct
:affectsHostProduct rdf:type owl:ObjectProperty ;
                    rdfs:domain :PackageEvent ;
                    rdfs:range <http://purl.org/cyber/cve#Product> ;
                    rdfs:comment "Indicates that a vulnerability or CVE affects a host-level product, such as an operating system, firmware, or appliance." .


###  http://www.semanticweb.org/ontology/2025/semlogonto/affectsPackage
:affectsPackage rdf:type owl:ObjectProperty ;
                owl:inverseOf :isAffectedBy ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( <http://purl.org/cyber/cve#CVE>
                                            <http://purl.org/cyber/cve#Vulnerability>
                                          )
                            ] ;
                rdfs:range :Package ;
                rdfs:comment """Indicates that a specific product or package is affected by a given vulnerability (CVE).
This property is used as the inverse of vulnerability-to-product relations, enabling queries such as “Which vulnerabilities affect this package?”.
It can function as a unified inverse relation covering both product-level and package-level impact associations.""" .


###  http://www.semanticweb.org/ontology/2025/semlogonto/authenticateTo
:authenticateTo rdf:type owl:ObjectProperty ;
                rdfs:domain <http://xmlns.com/foaf/0.1/Agent> ;
                rdfs:range :Asset ;
                rdfs:comment "Authentication relationship. Useful for: brute-force, auth failure evidence." .


